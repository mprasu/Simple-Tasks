#Using string

s=input("enter your string")
if s[::1]==s[::-1]: 
  print("palindrome")
else:
  print("not palindrome")
  
#using numbers

num=int(input("enter your number"))
temp=num
new=0
while num>0:
  rem=num%10
  new=new*10+rem
  num=num//10
if(temp==new):
  print("palindrome")
else:
  print("not palindrome")
  
#Reverse of a number
num=int(input("enter your number"))
temp=num
new=0
while num>0:
  rem=num%10
  new=new*10+rem
  num=num//10
print("reverse of a number is:",new)
